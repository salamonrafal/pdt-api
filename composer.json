{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "7.1.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "gufy/pdftohtml-php": "2.0.8",
        "phpoffice/phpword": "0.15.0",
        "sensio/framework-extra-bundle": "5.2.0",
        "symfony/config": "3.4.14",
        "symfony/console": "3.4.14",
        "symfony/flex": "1.0.89",
        "symfony/framework-bundle": "3.4.14",
        "symfony/http-foundation": "3.4.14",
        "symfony/lts": "3",
        "symfony/property-access": "3.4.14",
        "symfony/serializer": "3.4.14",
        "symfony/yaml": "3.4.14"
    },
    "require-dev": {
        "symfony/dotenv": "3.4.14",
        "symfony/test-pack": "1.0.2",
        "symfony/web-server-bundle": "3.4.14"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PDT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PDT\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
